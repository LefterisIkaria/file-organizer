{
  "active": true,
  "directory": "/path/to/dir",
  "categories": [
    {
      "name": "Documents",
      "extensions": [
        ".doc",
        ".docx",
        ".pdf",
        ".txt",
        ".odt",
        ".xls",
        ".xlsx",
        ".ppt",
        ".pptx"
      ],
      "categorize_extensions": true
    },
    {
      "name": "Images",
      "extensions": [
        ".jpg",
        ".jpeg",
        ".png",
        ".gif",
        ".bmp",
        ".tiff",
        ".ico",
        ".svg"
      ],
      "categorize_extensions": true
    },
    {
      "name": "Music",
      "extensions": [".mp3", ".wav", ".flac", ".m4a", ".aac", ".ogg"],
      "categorize_extensions": false
    },
    {
      "name": "Videos",
      "extensions": [".mp4", ".mkv", ".flv", ".mov", ".avi", ".wmv"],
      "categorize_extensions": false
    },
    {
      "name": "Archives",
      "extensions": [".zip", ".rar", ".tar", ".gz", ".7z"],
      "categorize_extensions": true
    },
    {
      "name": "Code",
      "extensions": [
        ".py",
        ".js",
        ".html",
        ".css",
        ".java",
        ".c",
        ".cpp",
        ".go",
        ".rb",
        ".php"
      ],
      "categorize_extensions": true
    },
    {
      "name": "Database",
      "extensions": [".sql", ".db", ".sqlite", ".mdb"],
      "categorize_extensions": false
    },
    {
      "name": "Designs",
      "extensions": [".psd", ".ai", ".sketch", ".xd"],
      "categorize_extensions": false
    },
    {
      "name": "E-books",
      "extensions": [".epub", ".mobi"],
      "categorize_extensions": false
    },
    {
      "name": "Executables",
      "extensions": [".exe", ".msi", ".sh", ".bat"],
      "categorize_extensions": false
    }
  ],
  "schedule": {
    "type": "MINTUTE",
    "interval": 1,
    "active": false
  }
}
